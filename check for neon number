package hell;
import java.util.Scanner;
public class classhel {
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int m=n*n;
        int sum=0;
        while(m>0)
        {
            sum=sum+m%10;
            m=m/10;
        }
        if(sum==n)
        {
            System.out.println("the given number is a neon");
        }
        else
        {
            System.out.println("the given number is not a neon");
        }
    
    }
   
}
    

